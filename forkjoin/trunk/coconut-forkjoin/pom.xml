<?xml version="1.0" encoding="UTF-8"?>
<project>
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.coconut.forkjoin</groupId>
    <artifactId>coconut-forkjoin-parent</artifactId>
    <version>0.1</version>
    <relativePath>../pom.xml</relativePath>
  </parent>
  <artifactId>coconut-forkjoin</artifactId>
  <name>Coconut Forkjoin</name>
  <version>0.1</version>
  <packaging>jar</packaging>
  <distributionManagement>
    <site>
      <id>codehaus.org</id>
      <url>dav:https://dav.codehaus.org/coconut/forkjoin</url>
    </site>
  </distributionManagement>
  <dependencies>
    <dependency>
      <groupId>org.coconut.forkjoin</groupId>
      <artifactId>jsr166y</artifactId>
      <version>070108</version>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <configuration>
          <links>
            <link>http://java.sun.com/j2se/1.5.0/docs/api</link>
            <link>http://gee.cs.oswego.edu/dl/jsr166/dist/jsr166ydocs/</link>
          </links>
          <showPackage>false</showPackage>
          <showProtected>true</showProtected>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>